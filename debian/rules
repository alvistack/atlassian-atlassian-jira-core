#!/usr/bin/make -f

SHELL := /bin/bash

override_dh_auto_build:

override_dh_auto_install:
	install -Dpm755 -d debian/tmp/usr/lib/systemd/system
	install -Dpm755 -d debian/tmp/opt/atlassian/jira
	install -Dpm644 -t debian/tmp/usr/lib/systemd/system jira.service
	install -Dpm644 -t debian/tmp/opt/atlassian atlassian-jira-core.patch

override_dh_strip:

override_dh_strip_nondeterminism:

override_dh_shlibdeps:

override_dh_makeshlibs:

override_dh_dwz:

override_dh_auto_test:

override_dh_auto_clean:

%:
	dh $@
